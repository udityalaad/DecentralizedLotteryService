				},
				"changeOwner(address)": {
					"details": "Service owner can change the ownership",
					"params": {
						"newOwner": ": The new owner to transfer transfer the service control to"
					}
				},
				"checkProfits()": {
					"details": "Check unspent ownerProfits"
				},
				"declareLotteryResults(string)": {
					"details": "Lottery results can be declared only after the lottery-specific-deadline has passed      a. Winner is randomly decided      b. `winningAmount = MIN(lottery.totalValue, 2 * winnerContribution)` is transferred to the winner      c. +ve difference left, is added to owner's profit",
					"params": {
						"name": ": to identify the lottery by"
					}
				},
				"getLotteryInfo(string)": {
					"details": "returns relavant information about the specified lottery, such as:              a. name, value              b. if winner available/possible_to declare:  winner, winningAmount, resultDeclaredAt                 otherwise:                                when results can be declared ",
					"params": {
						"name": ": to identify the lottery by"
					}
				},
				"getOrDeclareLotteryWinner(string)": {
					"details": "if already available:     returns winner for the specified lottery      else if deadline passed:  declares the result & returns the winner      otherwise:                errors out",
					"params": {
						"name": ": to identify the lottery by"
					}
				},
				"getParticipantHistory(address)": {
					"details": "returns relavant information about participant's lottery history, such as:              a. Lotteries participated in              b. Lotteries won (if any)          with their specifics",
					"params": {
						"participant": ": to return the history for"
					}
				},
				"isLotteryNameAvailable(string)": {
					"details": "Check if the given name is available to be assigned for a new Lottery",
					"params": {
						"name": ": to check availability for"
					}
				},
				"participateInLottery(string)": {
					"details": "Any one can participate in a lottery (at most once per user/lottery) till the set deadline",
					"params": {
						"name": ": to identify the lottery by"
					}
				},
